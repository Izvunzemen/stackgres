{
  "metadata":{
    "name":"gcsfetch",
    "namespace":"ongres"
  },
  "spec":{
    "baseBackups":{
      "cronSchedule":"*/2 * * * *",
      "retention":5,
      "compression":"lz4",
      "performance":{
        "uploadDiskConcurrency":1
      }
    },
    "storage":{
      "gcs":{
        "bucket":"google",
        "gcpCredentials":{
          "fetchCredentialsFromMetadataService":true
        }
      },
      "type":"gcs"
    }
  }
}