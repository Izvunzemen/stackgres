{
  "metadata":{
    "name":"stackgres",
    "namespace":"stackgres"
  },
  "spec":{
    "postgres": {
      "version":"latest"
    },
    "replication": {
      "mode": "async",
      "role": "ha-read"
    },
    "instances":"1",
    "pods":{
      "persistentVolume":{
        "size":"1Gi"
      },
      "disableConnectionPooling":false,
      "disableMetricsExporter":false,
      "disablePostgresUtil":false
    }
  }
}